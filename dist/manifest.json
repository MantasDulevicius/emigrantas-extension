{
  "manifest_version": 2,
  "name": "EmiController",
  "description": "Enchances Emigrantas.lt video player functionality.",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.emigrantas.tv/player/*"
      ],
      "css": [
        "index.css"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "assets/fonts/*"
  ],
  "icons": {
    "16": "assets/img/icon.png",
    "48": "assets/img/icon.png",
    "128": "assets/img/icon.png"
  },
  "permissions": []
}